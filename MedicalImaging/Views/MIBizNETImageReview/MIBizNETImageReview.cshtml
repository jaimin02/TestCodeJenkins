    <link rel="stylesheet" href="@Url.Content("~/Contents/Themes/Css/datepicker3.css")">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="@Url.Content("~/Contents/Themes/Css/daterangepicker.css")">

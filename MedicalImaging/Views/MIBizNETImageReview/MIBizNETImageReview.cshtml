    <input type="hidden" id="hdnParentWorkSpaceId" value="">
    <input type="hidden" id="hdnPeriodId" value="">
    <input type="hidden" id="hdnActivityName" value="">
    <input type="hidden" id="hdnSubActivityName" value="">
    <input type="hidden" id="hdnsubinodeID" value="">
    <input type="hidden" id="hdnparentActivityID" value="">

    <div class="spinner" style="display: none">
        <div></div>
        <div></div>
        <div></div>
        <div></div>
    </div>


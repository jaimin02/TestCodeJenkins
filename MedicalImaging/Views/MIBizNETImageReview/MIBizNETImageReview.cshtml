  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">

    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>D</b>I</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>D</b>I</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->


                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->

                        <!-- User Account: style can be found in dropdown.less -->

                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Image Review<small></small></h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i>BizNet</a></li>
                    <li class="active">Dicom Image Review</li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">




                <!-- Small boxes (Stat box) -->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-header" style="text-align: right">
                                <div class="row">
                                    @*<button style="text-align: right" type="button" id="" class="btn btn-primary" title="Close" data-tooltip="tooltip" onclick="window.close();">Close</button>*@
                                    <button type="button" id="" class="btn margin btn-primary btndangereffect" title="CLOSE" data-tooltip="tooltip" onclick="window.close();">
                                        <span class="num">
                                            <i class="fa fa-close" style="line-height: 30px;"></i>
                                        </span>
                                        CLOSE
                                    <i class="faf fa fa-sign-in"></i>

                                    </button>
                                </div>
                            </div>

                            <!-- /.For Color Code -->
                            <div class="row" style="text-align: right;" id="legend">
                                <div class="col-xs-12 form-group">
                                    <div class="col-sm-12">
                                        <span>
                                            <canvas id="myCanvasOriginal" width="12" height="12" class="original"></canvas>
                                        </span>
                                        <span class="">Latest Image</span>
                                        <span>
                                            <canvas id="myCanvasReviewed" width="12" height="12" class="reviewed"></canvas>
                                        </span>
                                        <span class="">Previous Image</span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.For Color Code -->

                            <!-- /.box-header -->
                            <div class="box-body">
                                <table id="tblSubjectReviewList" class="table table-bordered table-striped dataTable">
                                </table>
                            </div>
                            <!-- /.box-body -->
                        </div>
                    </div>
                </div>
                <!-- /.row -->

            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0
            </div>
            <strong>Copyright &copy; 2016-2017 <a href="http://www.sarjen.com/">Sarjen Systems Pvt. Ltd.</a>.</strong> All rights
    reserved.
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                @*<li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>*@
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane" id="control-sidebar-home-tab">

                    <ul class="control-sidebar-menu">
                    </ul>
                    <!-- /.control-sidebar-menu -->
                    <ul class="control-sidebar-menu">
                    </ul>
                    <!-- /.control-sidebar-menu -->

                </div>
                <!-- /.tab-pane -->
                <!-- Stats tab content -->
                <div class="tab-pane" id="control-sidebar-stats-tab">Stats Tab Content</div>
                <!-- /.tab-pane -->
                <!-- Settings tab content -->
                <!-- /.tab-pane -->
                <input type="hidden" id="hdnuserid" value="@ViewBag.hdnuserid" />
                <input type="hidden" id="hdnUserNameWithProfile" value="@ViewBag.hdnUserNameWithProfile" />
            </div>
        </aside>
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <div class="modal fade" id="msgPopUp" role="dialog"></div>
    <!-- jQuery 2.2.3 -->
    @*<script src="@Url.Content("~/Contents/Themes/Js/jquery-2.2.3.min.js")"></script>*@

    <!-- jQuery 3.6.1 -->
    <script src="@Url.Content("~/Contents/Themes/Js/jquery-3.6.1.min.js")"></script>

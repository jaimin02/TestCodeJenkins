            function onUpdating(sender, args) {
                if (showProgress) {
                    var updateProgressDiv = $get('updateProgress');
                    createDiv();
                    updateProgressDiv.style.display = '';
                    $('#aspnetForm').attr("onkeypress", "javascript:return WebForm_FireDefaultButton(event, 'ctl00_btnDefault')");
                    setLayerPosition();
                }
            }

            function createDiv() {
                var divTag = document.createElement('div');

                divTag.id = 'shadow';
                divTag.setAttribute('align', 'center');
                divTag.style.position = 'absolute';
                divTag.style.top = '0px';
                divTag.style.left = '0px';
                divTag.style.opacity = '0.6';
                divTag.style.filter = 'alpha(opacity=30)';
                divTag.style.backgroundColor = '#000000';
                document.body.appendChild(divTag);
            }

            function onUpdated(sender, args) {
                // get the update progress div
                var updateProgressDiv = $get('updateProgress');
                // make it invisible
                updateProgressDiv.style.display = 'none';

                if (document.getElementById('shadow') != null) {
                    document.body.removeChild(document.getElementById('shadow'));
                }
                $('#aspnetForm').removeAttr('onkeypress');
                //clearTimeout(tld);
            }

            function setLayerPosition() {

                var winScroll = BodyScrollHeight();
                var updateProgressDivBounds = Sys.UI.DomElement.getBounds($get('updateProgress'));
                var shadow = document.getElementById('shadow');
                var bws = GetWindowBounds();

                if (!shadow) {
                    return;
                }

                //shadow.style.width = bws.Width + "px";
                //shadow.style.height = bws.Height + "px";
                shadow.style.width = $(document).width().toString() + "px";
                shadow.style.height = $(document).height().toString() + "px";
                shadow.style.top = winScroll.yScr;
                shadow.style.left = winScroll.xScr;

                x = Math.round(($(document).width() - updateProgressDivBounds.width) / 2);
                y = Math.round(($(document).height() - updateProgressDivBounds.height) / 2);

                x += winScroll.xScr;
                y += winScroll.yScr;

                Sys.UI.DomElement.setLocation($get('updateProgress'), parseInt(x), parseInt(y));
            }
            window.onresize = setLayerPosition;
            window.onscroll = setLayerPosition;



                border-color: #4080fa;
                outline: none;
            }

            textarea:disabled, input[type='password']:disabled, input[type='text']:disabled, input[type='url']:disabled, input:not([type]):disabled {
                background: #f8f8f8;
                border-color: #c0c0c0;
            }
        /* CHECKBOX, RADIO */ input[type=checkbox], input[type=radio] {

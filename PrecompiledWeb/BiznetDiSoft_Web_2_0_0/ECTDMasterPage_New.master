            }

            textarea:focus, input[type='password']:focus, input[type='text']:focus, input[type='url']:focus, input:not([type]):focus {
                -webkit-box-shadow: 0 2px 2px rgba(0, 0, 0, .15), 0 1px 0 transparent, 0 0 1px #c0c0c0, 0 0 1px #c0c0c0, 0 0 1px #c0c0c0;

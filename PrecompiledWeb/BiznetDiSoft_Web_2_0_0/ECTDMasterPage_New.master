        /* TEXT */ textarea, input[type='password'], input[type='text'], input[type='url'], input:not([type]) {
            font-size: inherit;
            -webkit-transition: -webkit-box-shadow 100ms, background 100ms, border 500ms;
            -moz-transition: -moz-box-shadow 100ms, background 100ms, border 500ms;
            transition: box-shadow 100ms, background 100ms, border 500ms;
            border-radius: 3px;
            border: 1px solid #a0a0a0;
            font-size: 13px;
            font-weight: 400;
            margin: 0;
            padding: 3px;
        }

            textarea:hover, input[type='password']:hover, input[type='text']:hover, input[type='url']:hover, input:not([type]):hover {
